<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="512" height="512" fill="#080a0f" rx="80"/>
  
  <!-- Glow effect -->
  <defs>
    <radialGradient id="glow" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#d69e2e;stop-opacity:0.3"/>
      <stop offset="100%" style="stop-color:#d69e2e;stop-opacity:0"/>
    </radialGradient>
    <filter id="shadow">
      <feGaussianBlur in="SourceAlpha" stdDeviation="8"/>
      <feOffset dx="0" dy="4" result="offsetblur"/>
      <feComponentTransfer>
        <feFuncA type="linear" slope="0.5"/>
      </feComponentTransfer>
      <feMerge>
        <feMergeNode/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <circle cx="256" cy="256" r="200" fill="url(#glow)"/>
  
  <!-- Shop/Building icon -->
  <g filter="url(#shadow)">
    <!-- Roof -->
    <path d="M 256 120 L 380 200 L 380 220 L 132 220 L 132 200 Z" fill="#d69e2e"/>
    
    <!-- Building body -->
    <rect x="150" y="220" width="212" height="180" fill="#d69e2e" opacity="0.9"/>
    
    <!-- Door -->
    <rect x="220" y="310" width="72" height="90" fill="#080a0f" rx="4"/>
    
    <!-- Windows -->
    <rect x="170" y="250" width="40" height="40" fill="#080a0f" rx="3"/>
    <rect x="302" y="250" width="40" height="40" fill="#080a0f" rx="3"/>
    
    <!-- Beacon light rays -->
    <path d="M 256 80 L 246 120 L 266 120 Z" fill="#f6c94a" opacity="0.8"/>
    <path d="M 256 80 L 236 100 L 256 110 Z" fill="#f6c94a" opacity="0.6"/>
    <path d="M 256 80 L 276 100 L 256 110 Z" fill="#f6c94a" opacity="0.6"/>
    
    <!-- Star at top -->
    <circle cx="256" cy="80" r="12" fill="#f6c94a"/>
    <circle cx="256" cy="80" r="8" fill="#d69e2e"/>
  </g>
  
  <!-- Sparkles -->
  <circle cx="180" cy="160" r="3" fill="#f6c94a" opacity="0.8"/>
  <circle cx="332" cy="160" r="3" fill="#f6c94a" opacity="0.8"/>
  <circle cx="200" cy="380" r="2" fill="#f6c94a" opacity="0.6"/>
  <circle cx="312" cy="380" r="2" fill="#f6c94a" opacity="0.6"/>
</svg>
